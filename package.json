  "private": true,
  "license": "LGPL-3.0-only",
  "scripts": {
    "db:init": "yarn sequelize db:migrate && yarn sequelize db:seed:all",
    "db:migrate": "yarn sequelize db:migrate",
    "db:seed": "yarn sequelize db:seed:all",
    "db:undo": "yarn sequelize db:migrate:undo",
    "db:undo:all": "yarn sequelize db:migrate:undo:all",
    "db:mc": "yarn sequelize migration:create --name ",
    "db:sc": "yarn sequelize seed:create --name ",
    "dev": "yarn quasar dev -m electron",
    "build": "yarn && yarn sequelize db:migrate && yarn sequelize db:seed:all && yarn quasar clean && yarn quasar build -m electron -P always"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
